                "$Nullable": true
            },
            "City": {
                "$Nullable": true
            },
            "State": {
                "$Nullable": true
            },
            "ZipCode": {
                "$Nullable": true
            },
            "Country": {
                "$Nullable": true
            }
        },
        "DemoService": {
            "$Kind": "EntityContainer",
            "Products": {
                "$Collection": true,
                "$Type": "ODataDemo.Product"
            },
            "Categories": {
                "$Collection": true,
                "$Type": "ODataDemo.Category"
            },
            "Suppliers": {
                "$Collection": true,
                "$Type": "ODataDemo.Supplier"
            }
        }
