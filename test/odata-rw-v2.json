            },
            "Rating": {
                "$Type": "Edm.Int32"
            },
            "Price": {
                "$Type": "Edm.Decimal",
                "$Scale": 0
            },
            "Category": {
                "$Kind": "NavigationProperty",
                "$Nullable": true
            },
            "Supplier": {
                "$Kind": "NavigationProperty",
                "$Nullable": true
            }
        },
        "Category": {
            "$Kind": "EntityType",
            "$Key": [
                "ID"
            ],
            "ID": {
                "$Type": "Edm.Int32"
            },
            "Name": {
                "$Nullable": true
            },
            "Products": {
                "$Kind": "NavigationProperty",
                "$Nullable": true
            }
        },
        "Supplier": {
            "$Kind": "EntityType",
            "$Key": [
                "ID"
            ],
            "ID": {
                "$Type": "Edm.Int32"
            },
            "Name": {
                "$Nullable": true
            },
            "Address": {
                "$Type": "ODataDemo.Address"
            },
            "Concurrency": {
                "$Type": "Edm.Int32"
            },
            "Products": {
                "$Kind": "NavigationProperty",
                "$Nullable": true
            }
        },
        "Address": {
            "$Kind": "ComplexType",
